\def\lwtype#1{\@ifnextchar\bgroup{\@lwtype{#1}\lwtype}{\@@lwtype{#1}}}
\def\@lwtype#1{\mathchoice{{\textstyle\wtypesym}}{\wtypesym}{\wtypesym}{\wtypesym}({\textstyle #1})\;}
